    "\n",
    "def occurance(string):\n",
    "    \n",
    "    occ={}\n",
    "    for i in string:\n",
    "        if i in occ:\n",
    "             occ[i]+= 1\n",
    "        else:\n",
    "             occ[i]=1\n",
    "    return occ\n",
    "\n",
    "string='google.com'  \n",
    "res=occurance(string)\n",
    "print(str(res))\n",
    "        "
