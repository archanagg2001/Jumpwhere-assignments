    "class Circle():\n",
    "    def __init__(self, radius):\n",
    "        self.r = radius\n",
    "\n",
    "    def area(self):\n",
    "        return self.r**2*3.14\n",
    "    \n",
    "    def perimeter(self):\n",
    "        return 2*self.r*3.14\n",
    "\n",
    "NewCircle = Circle(4)\n",
    "print(f\"Area =\",NewCircle.area())\n",
    "print(f\"Perimeter=\",NewCircle.perimeter())\n"
   ]
