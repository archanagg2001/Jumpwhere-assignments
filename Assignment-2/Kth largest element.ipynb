    "import heapq\n",
    "def kth_largest(arr,k):\n",
    "    arr=[-elem for elem in arr]\n",
    "    heapq.heapify(arr)\n",
    "    for i in range(k-1):\n",
    "        heapq.heappop(arr)\n",
    "    return -heapq.heappop(arr)\n",
    "arr=[1,6,7,8,9,4,5,0,2]\n",
    "result=kth_largest(arr,3)\n",
    "print(result)"
   ]
